@model List<TempListViewModel>
<div class="col-md-12">
	<table  class="table table-striped table-hover table-dark-header">
		<thead>
			<tr>
				<th>类别名称</th>
				<th>人数</th>
			</tr>
		</thead>
		<tbody>
			@foreach (var item in Model)
			{
				<tr>
					<td>@item.Name</td>
					<td>@item.Count</td>
				</tr>
			}
		</tbody>
	</table>

</div>