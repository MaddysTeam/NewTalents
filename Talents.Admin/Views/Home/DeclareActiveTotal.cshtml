@model List<DeclareActiveTotalViewModel>
<div class="row">
	<div class="col-md-12">
		<div class="widget">
			<div class="widget-header">
				<h3><i class="fa fa-tasks"></i> 活动年度分布</h3>
			</div>
			<div class="widget-content">
				<table class="table table-condensed table-hover">
					<thead>
						<tr>
							<th class="width-p-20">&nbsp;</th>
							<th>活动数量</th>
						</tr>
					</thead>
					<tbody>
						@foreach (var item in Model)
						{
							<tr>
								<td class="text-center">@item.Year.ToString("yyyy") 年</td>
								<td>@item.ActiveCount</td>
							</tr>
						}
					</tbody>
				</table>
			</div>
		</div>
	</div>
</div>
	
