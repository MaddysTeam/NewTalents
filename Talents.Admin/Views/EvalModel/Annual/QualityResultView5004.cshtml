	var PeixKec_KecZiyKaif = EngineManager.GetEvalResult(items, EvalQualityRuleKeys.PeixKec_KecZiyKaif);
	var PeixKec_Total = EvalHelper.GetScore(30, "", PeixKec_KaisJiaosPeixKec.ResultValue, PeixKec_JiangzBaog.ResultValue, PeixKec_KecZiyKaif.ResultValue);
	var DaijJiaos_XueyShul = EngineManager.GetEvalResult(items, EvalQualityRuleKeys.DaijJiaos_XueyShul);
	var DaijJiaos_DaijJih = EngineManager.GetEvalResult(items, EvalQualityRuleKeys.DaijJiaos_DaijJih);
	var DaijJiaos_XueyChengzFenx = EngineManager.GetEvalResult(items, EvalQualityRuleKeys.DaijJiaos_XueyChengzFenx);
	var DaijJiaos_DaijZhid = EngineManager.GetEvalResult(items, EvalQualityRuleKeys.DaijJiaos_DaijZhid);
	var DaijJiaos_KaizShik = EngineManager.GetEvalResult(items, EvalQualityRuleKeys.DaijJiaos_KaizShik);
	var DaijJiaos_FablunwHuocYukTiyJiu = EngineManager.GetEvalResult(items, EvalQualityRuleKeys.DaijJiaos_FablunwHuocYukTiyJiu);
	var DaijJiaos_JiaoyJiaoxPingb = EngineManager.GetEvalResult(items, EvalQualityRuleKeys.DaijJiaos_JiaoyJiaoxPingb);
	var DaijJiaos_Total = EvalHelper.GetScore(11, "", DaijJiaos_KaizShik.ResultValue, DaijJiaos_FablunwHuocYukTiyJiu.ResultValue, DaijJiaos_JiaoyJiaoxPingb.ResultValue);
