						<td>
							<span>@ZisFaz_Total1</span>分
						</td>
					</tr>
					<!--发表论文-->
					<tr>
						<td rowspan="3"></td>
						<td>发表论文</td>
						@if (ZisFaz_FabLunw.ChooseValue == "1A")
						{
							<td>达量质考</td>
							<td><label> A：高质量的学科论文，具有前沿、引领、指导性</label></td>
												<td><span>@ZisFaz_FabLunw.ResultValue</span>分</td>
						}
						else if (ZisFaz_FabLunw.ChooseValue == "1B")
						{
							<td>达量质考</td>
								<td><label> B：质量较高的学科论文，有一定的前沿、引领、指导作用</label></td>
													<td><span>@ZisFaz_FabLunw.ResultValue</span>分</td>
