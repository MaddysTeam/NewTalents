@model List<FindCompanyModel>
<table class="table table-condensed table-striped table-hover table-dark-header">
	<thead>
		<tr>
			<th>单位名称</th>
			<th class="width-p-20">操作</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>@item.companyName</td>
				<td>
					<div class="btn-group btn-group-xs pull-right" data-row-id="@item.companyId">
						<button class="btn btn-success btn-assign">指派</button>
					</div>
				</td>
			</tr>
		}
	</tbody>
</table>