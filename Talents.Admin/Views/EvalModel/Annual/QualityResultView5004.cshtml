							<td>减量质考</td>
								<td><label> B：没有进行此项工作；或者没有区级及以上相应的佐证材料</label></td>
													<td>0分</td>
						}
					</tr>
					<!--评比获奖-->
					<tr>
						<td>评比获奖</td>
						@if (ZisFaz_PingbHuoj.ChooseValue == "1A")
						{
							<td>达量质考</td>
								<td><label> A：负责的重大课题或项目获奖；或其他高水准的大奖</label></td>
													<td><span>@ZisFaz_PingbHuoj.ResultValue</span>分</td>
						}
						else if (ZisFaz_PingbHuoj.ChooseValue == "1B")
						{
							<td>达量质考</td>
								<td><label> B：主持或负责的课题或项目获奖；或其他专业水平高的奖项</label></td>
													<td><span>@ZisFaz_PingbHuoj.ResultValue</span>分</td>
						}
						else if (ZisFaz_PingbHuoj.ChooseValue == "1C")
