@model List<EvalQualityResult>
<table class="table table-striped table-bordered">
	<thead>
		<tr>
			<th>专家</th>
			<th>评分</th>
			<th>考评时间</th>
			<th>操作</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr class="text-left">
				<td>@item.AccesserName</td>
				<td>@string.Format("{0} / {1}", item.Score, item.FullScore)</td>
				<td>@item.AccessDate.ToString("yyyy-MM-dd")</td>
				<td class="text-right"><button class="btn btn-xs btn-warning btn-details" data-params="@item.ResultId,@item.TeacherId,@item.PeriodId">查看明细</button></td>
			</tr>
		}
	</tbody>
</table>