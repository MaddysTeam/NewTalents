							<td>达量质考</td>
								<td><label> C：作为主要参与者的课题或项目获奖；或其他专业水平较高的奖项</label></td>
													<td><span>@ZisFaz_PingbHuoj.ResultValue</span>分</td>
						}
						else if (ZisFaz_PingbHuoj.ChooseValue == "1D")
						{
							<td>达量质考</td>
								<td><label> D：参与的课题或项目获奖；或其他较高的奖项</label></td>
													<td><span>@ZisFaz_PingbHuoj.ResultValue</span>分</td>
						}
						else if (ZisFaz_PingbHuoj.ChooseValue == "2A")
						{
							<td>减量质考</td>
								<td><label> A：获得区际间联合举行的教育教学活动奖项，或者市级多部门（非市级官方组织）联合开展的活动奖项</label></td>
													<td><span>@ZisFaz_PingbHuoj.ResultValue</span>分</td>
						}
						else if (ZisFaz_PingbHuoj.ChooseValue == "2B")
						{
							<td>减量质考</td>
								<td><label> B：没有获奖；或者没有市级及以上相应的佐证材料</label></td>
													<td>0分</td>
