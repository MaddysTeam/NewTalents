						else if (ZisFaz_FabLunw.ChooseValue == "1C")
						{
							<td>达量质考</td>
								<td><label> C：质量较高的学科论文</label></td>
													<td><span>@ZisFaz_FabLunw.ResultValue</span>分</td>
						}
						else if (ZisFaz_FabLunw.ChooseValue == "1D")
