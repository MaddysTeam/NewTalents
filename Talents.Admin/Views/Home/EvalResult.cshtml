@model List<EvalCommentAndScoreViewModel>
<div class="row">
	<div class="col-md-12">

		<div class="widget">
			<div class="widget-header">
				<h3><i class="fa fa-tasks"></i> 2020学年度第一学期团队计划、个人计划考评结果</h3>
			</div>
			<div class="widget-content">

				<table class="table table-condensed table-hover">
					<thead>
					</thead>
					<tbody>
							@foreach (var item in Model)
							{
									<tr>
										<td><strong> @item.EvalTitle： @item.Score</strong></td>
									</tr>
									<tr>
										<td><strong> 评审意见： @item.EvalComment </strong></td>
									</tr>
								<tr>
									<td><p>&nbsp;</p></td>
								</tr>

							}		
					</tbody>
				</table>

			</div>
		</div>

	</div>
</div>



